{
  "name": "meal2list",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:test": "cross-env NODE_ENV=test ng serve --configuration=test",
    "generate-env": "node scripts/generate-env.js",
    "build": "npm run generate-env && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --watch",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cross-env NODE_ENV=test playwright test --ui",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "lint": "ng lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "test:ci": "ng test --no-watch --code-coverage --browsers=ChromeHeadless"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.6",
    "@angular/cdk": "^19.2.9",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.9",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@supabase/supabase-js": "^2.49.4",
    "dotenv": "^16.5.0",
    "ngx-skeleton-loader": "^11.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uri-js": "^4.4.1",
    "zod": "^3.24.3",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.7",
    "@angular-eslint/builder": "19.3.0",
    "@angular-eslint/eslint-plugin": "19.3.0",
    "@angular-eslint/eslint-plugin-template": "19.3.0",
    "@angular-eslint/schematics": "19.3.0",
    "@angular-eslint/template-parser": "19.3.0",
    "@angular/cli": "^19.2.7",
    "@angular/compiler-cli": "^19.2.0",
    "@playwright/test": "^1.52.0",
    "@testing-library/angular": "^17.3.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/dotenv": "^6.1.1",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.18.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "angular-eslint": "^19.3.0",
    "cross-env": "^7.0.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^10.1.2",
    "husky": "^9.1.7",
    "jasmine-core": "~5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.5.5",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "typescript": "~5.7.2"
  }
